# python program to find the largest elemtn in arary
def maxarray(arr,n):
    max = arr[0]
    for i in range(1,n):
        if (arr[i]>max):
            max=arr[i]
    return max
arr=[eval(e) for e in input("Enters the values for array :").split(",")]
n=len(arr)
print ("the maximum value of array is ",maxarray(arr,n))


